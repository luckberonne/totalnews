generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Noticias {
  id         Int       @id @default(autoincrement())
  titulo     String?   @db.VarChar(255)
  subtitulo  String?   @db.VarChar(255)
  lead       String?
  cuerpo     String?
  extra      String?
  autor      String?   @db.VarChar(100) @default("An√≥nimo")
  fecha_hora DateTime? @db.Timestamp(6) @default(now())
}
