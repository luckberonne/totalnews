generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Noticias{
  id         Int      @id @default(autoincrement())
  titulo     String
  subtitulo  String
  lead       String
  cuerpo     String
  extra      String
  autor      String
  fecha_hora DateTime @default(now())
}